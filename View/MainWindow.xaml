<Window x:Class="ElectionsProgram.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ElectionsProgram.View"
        xmlns:k="clr-namespace:ElectionsProgram.ViewModel"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"
        d:DataContext="{d:DesignInstance {x:Type k:ElectionsBase}}">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="40.004"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button Content="Button" HorizontalAlignment="Left" Margin="10,5,0,0" VerticalAlignment="Top" Click="Button_Click" Height="25" Width="97"/>
        <DataGrid Grid.Row="1" Name="dgMedias" ItemsSource="{Binding Mediaresources}"/>
        <TextBlock HorizontalAlignment="Left" Margin="124,10,0,0" TextWrapping="Wrap" Text="{Binding Description}" VerticalAlignment="Top"/>

    </Grid>
</Window>
